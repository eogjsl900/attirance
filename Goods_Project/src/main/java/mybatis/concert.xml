<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="concert.model.Concert">
	
	<select id="GetConcert" resultType="MyConcert">
		select * from concert 
		where day = #{day}
	</select>

</mapper>







